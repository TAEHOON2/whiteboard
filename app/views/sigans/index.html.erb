<div class="postbox">
  
  <p class="text1"> 팀플모임방</p>

<div id="wannawrite">
    <%= link_to '새 글 작성하기', new_sigan_path(:currentgwamokid=> @currentgwamokid)  %>
  </div>
  <table class="posttable">
  <thead>
    <tr>
      <th>작성자</th>
      <th>제목</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sigans.each do |sigan|
     if sigan.gwamokid == @currentgwamokid %>
      <tr>
        <td class="post-t"><%= sigan.username %></td>
        <td><%= sigan.title %></td>
        <td class="post-btn"><%= link_to '보기', sigan %></td>
        <td class="post-btn"><%= link_to '수정', edit_sigan_path(sigan) %></td>
        <td class="post-btn"><%= link_to '삭제', sigan, method: :delete, data: { confirm: '정말 삭제하시겠습니까?' } %></td>
      </tr>
      <% end%> 
    <% end %>
  </tbody>
  </table>


  
</div>
